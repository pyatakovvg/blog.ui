{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": true }],
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    ["module-resolver", {
      "alias": {
        "@models": "./src/models",
        "@repositories": "./src/repositories",
        "@services": "./src/services"
      }
    }]
  ]
}